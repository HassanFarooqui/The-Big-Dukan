@model TheBigDukan.Models.SignUpModel
@{
    ViewBag.Title = "VenderActionIndex";
}
<body>
    <div class="container">
        <div class="row">
            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4">
                <label  onclick="location.href='@Url.Action("LogInAction", "LogIn")'"> Login </label>
            </div>
            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 row text-center">
               <img src="~/Images/Butler Logo.png" style="height: 150px; width:100px" />
               
            </div>

            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 pull-right row text-right">
                
                    @Html.DisplayFor(model => model.Name) 
            </div>
        </div>
    </div>
    <div class="row">
       <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
           <img class="img-responsive" src="~/Images/Electronic.jpg" />
       </div>

        <div   class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" src="~/Images/Fruits.jpg" />
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" src="~/Images/Gerocery.jpg" />
        </div>
    </div>

    <div class="row">
        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" src="~/Images/HouseHold.jpg" />
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" src="~/Images/Vegetable.jpeg"  />
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            
           

                <img src="~/Images/icons8-pill-50 (1).png"  onclick="location.href='@Url.Action("SignUpView", "LogIn")'"/>
           
           
        </div>
    </div>
</body>



