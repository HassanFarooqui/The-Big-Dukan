@model TheBigDukan.Models.SignUpModel
@{
    ViewBag.Title = "VenderActionIndex";
}
<body>
    <div class="container">
        <div class="row">
            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4">
                <label  onclick="location.href='@Url.Action("LogInAction", "LogIn")'"> Login </label>
            </div>
            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 row text-center">
               <img src="~/Images/Butler Logo.png" style="height: 150px; width:100px" />
               
            </div>

            <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 pull-right row text-right">
                
                    @Html.DisplayFor(model => model.Name) 
            </div>
        </div>
    </div>


    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">The Big Dukan</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="#">Home</a></li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        Products
                        <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a href="#">Electronics</a></li>
                        <li><a href="#">House Holds</a></li>
                        <li><a href="#">Pharmacy</a></li>
                        <li><a href="#">Vagetables</a></li>
                        <li><a href="#">Fruits</a></li>
                        <li><a href="#">Cosmetics</a></li>
                    </ul>
                </li>
                <li><a href="#">FAQs</a></li>
                <li><a href="#">Contact Us</a></li>
            </ul>
        </div>
    </nav>




    <div class="row"  >
       <div  class="col-sm-4 col-lg-4 col-xs-4 col-md-4  center-block" style="margin:0 auto;">
           <img class="img-responsive" style="height: 100px; width: 100px" src="~/Images/ElectronicItems (1).png" /> 
           <p> Electronics </p>
       </div>

        <div   class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" style="height: 100px; width: 100px" src="~/Images/Fruits.png" />
            <p>Foods</p>
            
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" style="height: 100px; width: 100px" src="~/Images/Toys.png" />
            <p>Toys</p>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive"  style="height: 100px; width: 100px" src="~/Images/Medicines.png" />
            <p>Medicines</p>
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            <img class="img-responsive" style="height: 100px; width: 100px"  src="~/Images/Vegetables.png"  />
            <p>Vegetables</p>
        </div>

        <div class="col-sm-4 col-lg-4 col-xs-4 col-md-4 ">
            
                <img src="~/Images/ShoppingCart-50.png"  style="height: 100px; width: 100px" onclick="location.href='@Url.Action("SignUpView", "LogIn")'"/>
           
           
        </div>
    </div>
</body>



